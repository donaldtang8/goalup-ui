    "config": "^3.2.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-sslify": "^1.2.0",
    "express-validator": "^6.2.0",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.7",
    "mongoose-findorcreate": "^3.0.0",
    "node-sass": "^4.13.0",
    "path": "^0.12.7",
    "react-autosuggest": "^9.4.3",
